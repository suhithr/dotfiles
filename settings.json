{
    "vim.leader": "<space>",
    "vim.easymotion": true,
    // "vim.sneak": true,
    // "vim.incsearch": true,
    "vim.useSystemClipboard": true,
    "vim.useCtrlKeys": true,
    "vim.hlsearch": true,
    "vim.insertModeKeyBindings": [
        {
        "before": ["j", "j"],
        "after": ["<Esc>"]
        }
    ],
    "vim.normalModeKeyBindingsNonRecursive": [
        {
        "before": ["<leader>", "d"],
        "after": ["d", "d"]
        },
        {
        "before": ["<C-n>"],
        "commands": [":nohl"]
        },
        {
        "before": ["<leader>", "<leader>"],
        "after": ["<leader>", "w"]
        },
        {
        "before": ["s"],
        "after": ["leader", "leader", "s"]
        },
        {
        "before": ["f"],
        "after": ["leader", "leader", "f"]
        }
    ],
    "vim.handleKeys": {
        "<C-a>": false,
        "<C-f>": false
    },
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,
    "workbench.statusBar.feedback.visible": false,
    "workbench.colorTheme": "Monokai Dimmed",
    "peacock.favoriteColors": [
        {
            "name": "Angular Red",
            "value": "#b52e31"
        },
        {
            "name": "Auth0 Orange",
            "value": "#eb5424"
        },
        {
            "name": "Azure Blue",
            "value": "#007fff"
        },
        {
            "name": "C# Purple",
            "value": "#68217A"
        },
        {
            "name": "Gatsby Purple",
            "value": "#639"
        },
        {
            "name": "Go Cyan",
            "value": "#5dc9e2"
        },
        {
            "name": "Java Blue-Gray",
            "value": "#557c9b"
        },
        {
            "name": "JavaScript Yellow",
            "value": "#f9e64f"
        },
        {
            "name": "Mandalorian Blue",
            "value": "#1857a4"
        },
        {
            "name": "Node Green",
            "value": "#215732"
        },
        {
            "name": "React Blue",
            "value": "#00b3e6"
        },
        {
            "name": "Something Different",
            "value": "#832561"
        },
        {
            "name": "Vue Green",
            "value": "#42b883"
        }
    ],
    "peacock.surpriseMeOnStartup": true,
    "editor.minimap.enabled": false,
    "python.linting.pylintEnabled": true,
    "python.linting.pylintArgs": [
        "--enable=W0614"
    ],
    "python.linting.enabled": false,
    "window.restoreWindows": "all",
    "gitlens.advanced.messages": {
        "suppressFileNotUnderSourceControlWarning": true
    },
    "html.suggest.html5": false,
    "[bazel]": {
        "editor.defaultFormatter": "BazelBuild.vscode-bazel"
    },
    "todo-tree.tree.showScanModeButton": false,
    "python.languageServer": "Default",
    // "bazel.buildifierExecutable": "/home/suhith.rajesh/development/github.robot.car/cruise/cruise/develop/tools/buildifier",
    // "bazel.buildifierFixOnFormat": true
    "window.restoreWindows": "all",
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter.notebook.ipynb"
    },
    "editor.lineNumbers": "on",
    "editor.wordWrap": "off",
    "editor.foldingStrategy": "indentation",
    "editor.stickyScroll.enabled": false,
    "[cpp]": {
        "breadcrumbs.showArrays": true,
        "editor.wordBasedSuggestions": "on",
        "editor.semanticHighlighting.enabled": true,
        "editor.stickyScroll.defaultModel": "foldingProviderModel",
        "editor.suggest.insertMode": "replace"
    },
    "liveshare.notebooks.allowGuestExecuteCells": true,
    "C_Cpp.intelliSenseEngine": "disabled",
    "makefile.configureOnOpen": true,
    "[cuda-cpp]": {

        "editor.wordBasedSuggestions": "off",
        "editor.semanticHighlighting.enabled": true,
        "editor.stickyScroll.defaultModel": "foldingProviderModel",
        "editor.suggest.insertMode": "replace"
    },
    "files.associations": {
        "*.cu": "cuda-cpp"
    },
    "remote.SSH.useExecServer": false,
    "remote.SSH.useLocalServer": false,
    "remote.SSH.remotePlatform": {
        "crosby.cluster.recurse.com": "linux"
    },
    "files.autoSave": "afterDelay",
}